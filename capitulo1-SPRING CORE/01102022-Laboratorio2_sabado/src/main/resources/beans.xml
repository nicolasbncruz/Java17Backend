<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- Ideal Para Ambientes Onpremise-->
    
    <bean id="holaMundoBean" class="edu.cibertec.capitulo1.beans.HolaMundo" >
        <property name="nombre" value="Alumnos Backend" />
    </bean>
    <bean id="impresoraServiceBean" class="edu.cibertec.capitulo1.service.ImpresoraService" >
        <property name="documento" ref="documentoExcel" />
    </bean>
    <bean id="documentoExcel" class="edu.cibertec.capitulo1.beans.DocumentoTextoExcel" >
    </bean>
</beans>
